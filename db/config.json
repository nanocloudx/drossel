{
  "development": {
    "database": "drossel",
    "host": "localhost",
    "dialect": "postgres",
    "use_env_variable": "DATABASE_URL",
    "dialectOptions": {
      "timezone": "+09:00"
    }
  },
  "test": {
    "use_env_variable": "DATABASE_URL",
    "dialectOptions": {
      "timezone": "+09:00"
    }
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialectOptions": {
      "ssl": {
        "require": true
      },
      "timezone": "+09:00"
    }
  }
}
